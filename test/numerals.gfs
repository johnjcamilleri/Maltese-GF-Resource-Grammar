cc -table -unqual num (pot3 (pot2plus pot01 pot111)) -- 111,000
cc -table -unqual num (pot3 (pot2plus pot01 (pot1 n3))) -- 130,000
cc -table -unqual num (pot3 (pot2plus pot01 (pot1plus n4 pot01))) -- 141,000
cc -table -unqual num (pot3 (pot2plus pot01 (pot1 n6))) -- 160,000
