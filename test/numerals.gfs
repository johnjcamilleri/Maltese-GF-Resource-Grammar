-- Treebank numerals test
-- Run using the following command:
-- ./_gf < test/numerals.gfs > test/numerals.out

se utf8
i -retain NumeralMlt.gf

cc -table -unqual num (pot3 (pot1as2 (pot0as1 pot01))) -- 1,000
cc -table -unqual num (pot3 (pot1as2 (pot0as1 (pot0 n2)))) -- 2,000
cc -table -unqual num (pot3 (pot1as2 (pot0as1 (pot0 n3)))) -- 3,000
cc -table -unqual num (pot3 (pot1as2 (pot0as1 (pot0 n9)))) -- 9,000
cc -table -unqual num (pot3 (pot1as2 pot110)) -- 10,000
cc -table -unqual num (pot3 (pot1as2 pot111)) -- 11,000
cc -table -unqual num (pot3 (pot1as2 (pot1to19 n2))) -- 12,000
cc -table -unqual num (pot3 (pot1as2 (pot1to19 n3))) -- 14,000
cc -table -unqual num (pot3 (pot1as2 (pot1to19 n9))) -- 19,000
cc -table -unqual num (pot3 (pot1as2 (pot1 n2))) -- 20,000
cc -table -unqual num (pot3 (pot1as2 (pot1plus n2 pot01))) -- 21,000
cc -table -unqual num (pot3 (pot1as2 (pot1plus n2 (pot0 n2)))) -- 22,000
cc -table -unqual num (pot3 (pot1as2 (pot1 n3))) -- 30,000
