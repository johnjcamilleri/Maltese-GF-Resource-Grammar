mkNP (mkNumeral n2_Unit) (man_N) -- two men
mkNP (mkNumeral "35") (man_N) -- thirty-five men
mkNP (mkNumeral (mkSub100 n2_Unit n1_Unit)) (man_N) -- twenty-one men
